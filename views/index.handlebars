<div>
    <h1 class="card z-depth-3 center align">Read the Latest News Articles </h1>
    <div class="row">
        <div class="col s3"></div>
        <h5 class="card z-depth-3 center align col s6"></h5>
    </div>
</div>
<div class="row">
    <div class="col s8 left align">
        {{#each article}}
        <div class="card-panel">
            <h4>{{this.title}}</h4>
            <a href="/readArticle/{{this.id}}" class="wave-effect" id="btn">READ ARTICLE</a>

        </div>

        {{/each}}
    </div>
</div>